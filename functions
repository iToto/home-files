##################
# BASH FUNCTIONS #
##################
purgeOrphanBranches() {
    git checkout master
    git fetch --all
    git merge origin/master
    git branch --merged | xargs git branch -d
    git checkout -
}

rebaseOffLatest() {
    git fetch --all
    git rebase origin/$@
}

updateBranchFromOrigin() {
    git checkout $@
    git fetch --all
    git merge origin/$@
    git checkout -
}
